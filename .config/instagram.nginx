server {
    # 80번 포트로 온 요청에 응답할 Block이다
    listen 80;

    # HTTP요청의 Host값 (URL에 입력한 도메인)
    server_name 15.164.226.135;

    charset utf-8;

    #root로부터의 요청에 대해 응답할 Block
    location /static/ {
            root /srv/instagram/app;
        }

    location / {
        # /run/gunicorn.sock파일을 사용해서Gunicorn에 소켓통신하는 proxy 꾸성
        proxy_pass      http://unix:/run/instagram.sock;
    }

}